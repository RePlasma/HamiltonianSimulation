Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"X", ",", " ", "Y"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Clear", "[", 
    RowBox[{
    "\[Delta]t", ",", "X", ",", "Y", ",", "U1", ",", "U2", ",", "U3", ",", 
     " ", "diff2", ",", "diff3"}], "]"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{"X", ",", "Y"}], "}"}], "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"PauliMatrix", "[", "1", "]"}], ",", 
       RowBox[{"PauliMatrix", "[", "2", "]"}]}], "}"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"X", ".", "Y"}], "-", 
     RowBox[{"Y", ".", "X"}]}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"first", " ", "order", " ", "Suzuki"}], "-", 
     RowBox[{"Trotter", " ", "decomposition"}]}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"U1", "=", 
     RowBox[{"MatrixExp", "[", 
      RowBox[{
       RowBox[{"-", "I"}], 
       RowBox[{"(", 
        RowBox[{"X", "+", "Y"}], ")"}], "\[Delta]t"}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"U2", "=", 
     RowBox[{
      RowBox[{"MatrixExp", "[", 
       RowBox[{
        RowBox[{"-", "I"}], 
        RowBox[{"(", "X", ")"}], "\[Delta]t"}], "]"}], ".", 
